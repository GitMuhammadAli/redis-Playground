  "main": "index.js",
  "scripts": {
    "redis:up": "docker run -d --name redis-playground-stack -p 6381:6379 -p 5540:8001 redis/redis-stack:latest",
    "redis:down": "docker rm -f redis-playground-stack",
    "redis:logs": "docker logs -f redis-playground-stack",
    "redis:ping": "docker exec -it redis-playground-stack redis-cli PING",
    "redis:cli": "docker exec -it redis-playground-stack redis-cli",
    "run:client": "node client.js",
    "run:queue": "node stack-qeue.js",
    "run:string": "node string.js",
    "run:bike": "node bike.js"
  },
